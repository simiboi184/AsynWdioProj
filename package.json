{
  "name": "asynprojects",
  "version": "1.0.0",
  "description": "Learning Async Mode for Webdriver IO",
  "main": "index.js",
  "dependencies": {
    "@wdio/cli": "^7.14.1",
    "npm": "^8.1.0",
    "webdriver-manager": "^12.1.8"
  },
  "scripts": {
    "test": "npx wdio wdio.conf.ts",
    "steps": "npx cucumber-js.cmd",
    "loginonly_test": "npx wdio wdio.conf.ts --suite loginonly",
    "smoke_test": "npx wdio wdio.conf.ts --suite smoke",
    "regression_test": "npx wdio wdio.conf.ts --suite regression",
    "mainworkflow_test": "npx wdio wdio.conf.ts --suite mainworkflow",
    "allure_test_report": "npx allure generate allure-results --clean && allure open"
  },
  "keywords": [
    "webdriver.io",
    "typescript",
    "cucumber",
    "node.js"
  ],
  "devDependencies": {
    "@types/supertest": "^2.0.11",
    "@wdio/allure-reporter": "^7.14.1",
    "@wdio/cucumber-framework": "^7.14.1",
    "@wdio/local-runner": "^7.14.1",
    "@wdio/spec-reporter": "^7.14.1",
    "allure-commandline": "^2.13.8",
    "chromedriver": "^94.0.0",
    "supertest": "^6.1.6",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4",
    "wdio-chromedriver-service": "^7.2.2",
    "webdriverio": "^7.14.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
